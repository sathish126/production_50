# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/production50
REDIS_URL=redis://localhost:6379

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=7d

# Email Configuration
